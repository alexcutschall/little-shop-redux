<!-- views/items/index.erb -->
<h1>Items</h1>
<form action='/items/new'>
   <input type='submit' value='Create a new item'/>
</form>
    <% @items.each do |item| %>
    <ul>
      <li><%= item.title %></li>
      <li><%= item.description %></li>
      <li><%= item.price %></li>
      <li><%= item.image %></li>
    </ul>
    <% end %>
